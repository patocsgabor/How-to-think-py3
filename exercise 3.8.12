# Write a program to draw a face of a clock:

import turtle

window = turtle.Screen()
window.bgcolor("lightgreen")

clock = turtle.Turtle()
clock.shape("turtle")
clock.color("blue")

clock.penup()

for i in range(12):
    clock.forward(50)
    clock.pendown()
    clock.forward(30)
    clock.penup()
    clock.forward(20)
    clock.stamp()
    clock.backward(100)
    clock.left(30)

window.mainloop()
